<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<%= link_to 'Create', new_article_path %>
<table><tr><th>Title</th></tr>
	<% @articles.each do |article| %>
		<tr><td><%= article.title %></td>
		<td><%= article.text %></td></tr>
		<td><%= link_to 'Show', article_path(article) %></td>
		<td><%= link_to 'Edit', edit_article_path(article) %>
		<td><%= link_to 'Delete', article_path(article), method: :delete, data: {confirm: "sure?"} %>
	<% end %>
</table>
